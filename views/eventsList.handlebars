<div class='menu-container'>
  <div class='card-header col-md-10'>
    <h2>
      &#127869 Upcoming Events
    </h2>
  </div>
  {{#each events as |event|}}
    <div class='dish col-md-5'>
      <p>
        {{event.event_id}}.
        {{event.event_name}}
      </p>
    </div>
    <div class='col-md-7'>
      <p>
        Host:
        {{event.user_name}}
      </p>
    </div>
  {{/each}}
</div>
